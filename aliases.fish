# Git aliases see ~/.gitconfig or ~/.config/git
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias go='git checkout '
alias gh='git hist'
alias gl='git log'

# ls aliases
alias ll='set LC_COLLATE C; ls -lhG'
alias la='set LC_COLLATE C; ls -lahG'

# vim aliases
alias mvim='nvim'
alias nvim='nvim'
alias vi='/usr/bin/vim'
alias vim='nvim'
